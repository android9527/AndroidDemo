<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.rey.material.demo">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme"
        android:supportsRtl="true">

        <activity
            android:name="com.rey.material.UIActivity"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustPan">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity
            android:name="com.demo.toolbar.ToolbarActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.rey.material.demo.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.demo.toolbar.ToolbarActivity2"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="no.agens.depth.RootActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.flyco.indicatorsamples.ui.SimpleHomeActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.flyco.indicatorsamples.ui.RoundCornerIndicaorActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.flyco.indicatorsamples.ui.FlycoPageIndicaorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.rd.pageindicatorview.home.PageIndicatorActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.pitt.freshdownloadview.FreshDownloadViewMainActivity"
                  android:screenOrientation="portrait"/>
    </application>

</manifest>
